{
  "name": "edfjs",
  "version": "0.2.0",
  "description": "A javascript implementation of the European Data Format.",
  "main": "src/index.js",
  "unpkg": "dist/edf.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "node ./scripts/pretest.js",
    "test": "mocha --reporter spec",
    "build": "browserify ./src/index.js --s edfjs -o ./dist/edf.js && uglifyjs ./dist/edf.js -mc > ./dist/edf.min.js",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jusjusjus/edfjs.git"
  },
  "author": "Justus Schwabedal <jschwabedal@gmail.com> (https://jusjusjus.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jusjusjus/edfjs/issues"
  },
  "watch": {
    "build": "src/*js",
    "test": [
      "src/*js",
      "test/*.js"
    ]
  },
  "homepage": "https://github.com/jusjusjus/edfjs#readme",
  "devDependencies": {
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "chai-almost": "^1.0.1",
    "filereader": "^0.10.3",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "mocha": "^5.2.0",
    "npm-watch": "^0.3.0"
  },
  "dependencies": {}
}
