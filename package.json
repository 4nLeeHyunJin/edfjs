{
  "name": "edfjs",
  "version": "0.3.1",
  "description": "A javascript implementation of the European Data Format.",
  "main": "src/index.js",
  "unpkg": "dist/edf.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "eslint src/*.js test/*.js && mocha --reporter spec",
		"build": "webpack --config ./config/webpack.config.js",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jusjusjus/edfjs.git"
  },
  "author": "Justus Schwabedal <jschwabedal@gmail.com> (https://jusjusjus.github.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jusjusjus/edfjs/issues"
  },
  "watch": {
    "build": "src/*js",
    "test": [
      "src/*js",
      "test/*.js"
    ]
  },
  "homepage": "https://github.com/jusjusjus/edfjs#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-almost": "^1.0.1",
    "chai-datetime": "^1.6.0",
    "eslint": "^7.1.0",
    "filereader": "^0.10.3",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "mocha": "^5.2.0",
    "npm-watch": "^0.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {}
}
